        </activity>
        <activity
            android:name=".Activities.ReportListActivity"
            android:label="@string/title_activity_report_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.NewPatioActivity" />
        <activity android:name=".Activities.NewTransporteActivity" />
        <activity android:name=".Activities.ArvoreActivity"></activity>
    </application>

</manifest>